    "@keystonejs/auth-password": "^6.1.1",
    "@keystonejs/fields": "^22.1.2",
    "@keystonejs/fields-auto-increment": "^8.2.1",
    "@keystonejs/fields-content": "^9.1.1",
    "@keystonejs/file-adapters": "^7.1.1",
    "@keystonejs/keystone": "^19.3.2",
    "@mdi/font": "^5.9.55",
    "@nuxtjs/auth-next": "5.0.0-1622018627.48534b6",
    "@nuxtjs/axios": "^5.13.1",
