    "@keystonejs/fields-auto-increment": "^8.2.2",
    "@keystonejs/fields-content": "^9.1.2",
    "@keystonejs/file-adapters": "^7.1.2",
    "@keystonejs/keystone": "^19.3.3",
    "@mdi/font": "^5.9.55",
    "@nuxtjs/auth-next": "5.0.0-1622226002.a93068f",
    "@nuxtjs/axios": "^5.13.5",
    "@popperjs/core": "^2.9.2",
