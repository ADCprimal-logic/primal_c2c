{
  "name": "c2c-children-connect",
  "description": "Proprietary Dashboard Software for C2C Kids",
  "private": true,
  "version": "0.2.0",
  "author": "primal. Development Team",
  "scripts": {
    "dev": "cd server && cross-env NODE_ENV=development DISABLE_LOGGING=true keystone dev",
    "build": "cd server && cross-env NODE_ENV=production keystone build",
    "start": "cd server && cross-env NODE_ENV=production keystone start",
    "build-test": "cd server && cross-env NODE_ENV=testing keystone build",
    "start-test": "cd server && cross-env NODE_ENV=testing keystone start",
    "create-tables": "cd server && cross-env keystone create-tables",
    "matchmaker": "cd server && cross-env keystone upgrade-relationships",
    "matchmaker-date": "cd server && cross-env keystone upgrade-relationships --migration",
    "prod-log": "yarn build && nodemon"
  },
  "dependencies": {
    "@keystonejs/adapter-knex": "^13.3.1",
    "@keystonejs/app-admin-ui": "^7.5.1",
    "@keystonejs/app-graphql": "^6.3.1",
    "@keystonejs/app-nuxt": "^5.2.2",
    "@keystonejs/auth-password": "^6.1.1",
    "@keystonejs/fields": "^22.1.2",
    "@keystonejs/fields-authed-relationship": "^1.1.1",
    "@keystonejs/fields-auto-increment": "^8.2.1",
    "@keystonejs/fields-color": "^1.1.1",
    "@keystonejs/fields-content": "^9.1.1",
    "@keystonejs/file-adapters": "^7.1.1",
    "@keystonejs/keystone": "^19.3.2",
    "@mdi/font": "^5.9.55",
    "@nuxtjs/auth-next": "^5.0.0-1620457172.0cf714c",
    "@nuxtjs/axios": "^5.13.1",
    "@vue-stripe/vue-stripe": "^4.1.8",
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "graphql": "^14.7.0",
    "nuxt": "^2.15.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "stripe": "^8.148.0",
    "twilio": "^3.62.0",
    "v-calendar": "^2.3.0",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.12",
    "vue-chartist": "^2.3.1",
    "vue-template-compiler": "^2.6.12",
    "vuetify": "^1.5.24",
    "vuetify-loader": "^1.7.2",
    "webpack": "^5.36.2"
  },
  "devDependencies": {
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.7",
    "sass-loader": "^8.0.2",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.3"
  }
}
